{
    "name": "neeckeloo/enum",
    "description": "Enum module for Zend Framework 2",
    "type": "library",
    "keywords": [
        "zf2", "enum"
    ],
    "license": "MIT",
    "homepage": "https://github.com/neeckeloo/Enum",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zend-servicemanager": "~2.5",
        "zendframework/zend-stdlib": "~2.5",
        "zendframework/zend-view": "~2.5"
    },
    "suggests": {
        "doctrine/doctrine-orm-module": "Allow using database via Doctrine ORM"
    },
    "autoload": {
        "psr-4": {
            "Enum\\": "src/Enum"
        },
        "classmap": [
            "./"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EnumTest\\": "tests/EnumTest"
        }
    }
}
